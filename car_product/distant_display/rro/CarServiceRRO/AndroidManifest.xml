<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.android.car.resources.distantdisplay.emulator">
    <application android:hasCode="false" />
    <!--
        Adapted all configuration from CarServiceOverlayEmulatorOsDouble and newly defined
        displayType=AUXILIARY_2 in config_occupant_display_mapping. Set higher priority than
        CarServiceOverlayEmulatorOsDouble(5001) to override rro configuration.
    -->
    <overlay android:priority="9001"
             android:targetPackage="com.android.car.updatable"
             android:targetName="CarServiceCustomization"
             android:resourcesMap="@xml/overlays"
             android:requiredSystemPropertyName="ro.emulator.car.distantdisplay"
             android:requiredSystemPropertyValue="true"
             android:isStatic="true" />
</manifest>
